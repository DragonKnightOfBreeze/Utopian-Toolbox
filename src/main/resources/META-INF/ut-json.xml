<idea-plugin>
    <extensions defaultExtensionNs="icu.windea.ut.toolbox">
        <jElementProvider language="JSON" implementationClass="icu.windea.ut.toolbox.lang.json.JsonJElementProvider"/>
    </extensions>

    <extensions defaultExtensionNs="com.intellij">
        <psi.referenceContributor language="JSON" implementation="icu.windea.ut.toolbox.lang.json.JsonLanguageSchemaBasedReferenceContributor"/>
        <annotator language="JSON" implementationClass="icu.windea.ut.toolbox.jast.LanguageSchemaBasedReferenceAnnotator"/>
        <lang.foldingBuilder language="JSON" implementationClass="icu.windea.ut.toolbox.lang.json.JsonLanguageSchemaBasedFoldingBuilder"/>
        <targetElementEvaluator language="JSON" implementationClass="icu.windea.ut.toolbox.jast.LanguageSchemaBasedTargetElementEvaluator"/>
        <completion.contributor language="JSON" implementationClass="icu.windea.ut.toolbox.lang.json.JsonLanguageSchemaBasedReferenceCompletionContributor"/>
    </extensions>
</idea-plugin>
