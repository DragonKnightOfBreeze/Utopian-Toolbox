<idea-plugin>
    <extensions defaultExtensionNs="icu.windea.ut.toolbox">
        <jElementProvider language="yaml" implementationClass="icu.windea.ut.toolbox.lang.yaml.YamlJElementProvider"/>
    </extensions>

    <extensions defaultExtensionNs="com.intellij">
        <psi.referenceContributor language="yaml" implementation="icu.windea.ut.toolbox.lang.yaml.YamlJsonPointerBasedReferenceContributor"/>
        <annotator language="yaml" implementationClass="icu.windea.ut.toolbox.jast.JsonPointerBasedReferenceAnnotator"/>
        <targetElementEvaluator language="yaml" implementationClass="icu.windea.ut.toolbox.jast.JsonPointerBasedTargetElementEvaluator"/>
        <completion.contributor language="yaml" implementationClass="icu.windea.ut.toolbox.lang.yaml.YamlJsonPointerBasedReferenceCompletionContributor"/>
    </extensions>
</idea-plugin>
